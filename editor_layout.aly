{
  LinearLayout;
  layout_width="fill";
  layout_height="fill";
  gravity="center";
  {
    ScrollView;
    layout_width="fill";
    VerticalScrollBarEnabled=false;
    layout_height="fill";
    {
      LinearLayout;
      layout_gravity="center|top";
      layout_height="fill";
      orientation="vertical";
      {
        CardView;
        CardElevation="5dp";
        layout_height="120dp";
        layout_width="fill";
        radius="10dp";
        layout_marginBottom="0dp";
        layout_gravity="left";
        background="#FFFFFF";
        layout_margin="20dp";
        {
          LinearLayout;
          layout_width="360dp";
          layout_height="140dp";
          gravity="center";
          layout_gravity="center";
          orientation="vertical";
          {
            ImageView;
            layout_width="30dp";
            layout_height="30dp";
            src="res/icon/modular.png";
          };
          {
            TextView;
            textSize="16sp";
            text="脚本工厂";
            textColor="#2196F3";
          };
        };
        {
          LinearLayout;
          layout_width="360dp";
          layout_height="140dp";
          gravity="center";
          layout_gravity="center";
          id="script_factory";
          style="?android:attr/buttonBarButtonStyle";
          orientation="vertical";
        };
      };
      {
        TextView;
        textColor="#2196F3";
        textSize="18sp";
        layout_margin="20dp";
        text="脚本开发工具";
      };
      {
        LinearLayout;
        layout_height="120dp";
        {
          CardView;
          CardElevation="5dp";
          layout_height="fill";
