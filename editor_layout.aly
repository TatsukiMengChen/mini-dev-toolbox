          CardView;
          CardElevation="5dp";
          background="#FFFFFF";
          layout_width="150dp";
          layout_height="fill";
          layout_marginBottom="0dp";
          layout_marginLeft="0dp";
          layout_gravity="right";
          layout_margin="20dp";
          radius="10dp";
