          layout_gravity="left";
          background="#FFFFFF";
          layout_margin="20dp";
          {
            LinearLayout;
            layout_width="160dp";
            layout_height="130dp";
            gravity="center";
            layout_gravity="center";
            id="script_editor";
            style="?android:attr/buttonBarButtonStyle";
            orientation="vertical";
            {
              ImageView;
              layout_width="30dp";
              layout_height="30dp";
              src="res/icon/code.png";
            };
            {
              TextView;
              textSize="16sp";
              text="脚本编辑器";
              textColor="#2196F3";
            };
          };
        };
        {
          CardView;
          CardElevation="5dp";
          background="#FFFFFF";
