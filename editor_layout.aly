            orientation="vertical";
            {
              ImageView;
              layout_width="30dp";
              layout_height="30dp";
              src="res/icon/lua.png";
            };
            {
              TextView;
              textSize="16sp";
              text="Lua编辑器";
              textColor="#2196F3";
            };
          };
        };
      };
      {
        LinearLayout;
        layout_height="120dp";
        {
          CardView;
          CardElevation="5dp";
          layout_height="fill";
          layout_width="150dp";
          radius="10dp";
          layout_marginBottom="0dp";
          layout_gravity="left";
          background="#FFFFFF";
          layout_margin="20dp";
