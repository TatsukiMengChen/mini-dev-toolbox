{
  LinearLayout;
  layout_width="fill";
  orientation="vertical";
  background="#FAFAFA";
  layout_height="fill";
  {
    LinearLayout;
    layout_height="50dp";
    layout_width="fill";
    {
      Button;
      onClick=function()activity.finish() end;
      background="res/icon/direction-left.png";
      layout_height="30dp";
      layout_width="30dp";
      layout_gravity="center";
      layout_margin="10dp";
    };
    {
      TextView;
      textColor="#2196F3";
      text="设置";
      layout_gravity="center";
      textSize="24sp";
    };
  };
  {
    ScrollView;
    layout_height="fill";
    layout_width="fill";
    {
      LinearLayout;
      orientation="vertical";
      layout_width="fill";
      layout_height="fill";
      {
        LinearLayout;
        orientation="vertical";
        layout_width="fill";
        {
          TextView;
          textColor="#2196F3";
          layout_marginBottom="0dp";
          text="Wiki模块";
          layout_margin="10dp";
          textSize="18sp";
        };
        {
          LinearLayout;
          orientation="horizontal";
          layout_width="fill";
          {
            EditText;
            maxLines="3";
            layout_marginLeft="5dp";
            id="wiki";
            text="https://developers.mini1.cn/wiki/API.html";
            layout_width="300dp";
            layout_marginRight="5dp";
            layout_gravity="center";
          };
          {
            Button;
            onClick="Wiki";
            layout_gravity="center";
            layout_width="30dp";
            background="res/icon/save.png";
            layout_height="30dp";
          };
        };
      };
      {
        LinearLayout;
        orientation="vertical";
        layout_width="fill";
        {
          TextView;
          textColor="#2196F3";
          layout_marginBottom="0dp";
          text="缓存清理";
          layout_margin="10dp";
          textSize="18sp";
        };
        {
          LinearLayout;
          orientation="horizontal";
          layout_width="fill";
          {
            TextView;
            id="cache_size";
            layout_marginLeft="5dp";
            text="0MB";
            textSize="18sp";
            layout_width="295dp";
            layout_marginLeft="10dp";
            layout_marginRight="5dp";
            layout_gravity="center";
          };
          {
            Button;
            layout_gravity="center";
            id="clean_cache";
            layout_width="30dp";
            background="res/icon/ashbin.png";
            layout_height="30dp";
          };
        };
      };
    };
  };
};
