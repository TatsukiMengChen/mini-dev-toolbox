{
  LinearLayout;
  layout_width="fill";
  gravity="center";
  layout_height="fill";
  {
    ScrollView;
    layout_height="fill";
    layout_width="fill";
    VerticalScrollBarEnabled=false;
    {
      LinearLayout;
      layout_gravity="center|top";
      orientation="vertical";
      layout_width="fill";
      layout_height="fill";
      gravity="center|top";
      {
        CardView;
        layout_margin="20dp";
        radius="20dp";
        layout_width="320dp";
        layout_height="100dp";
        background="#FFFFFF";
        {
          LinearLayout;
          style="?android:attr/buttonBarButtonStyle";
          orientation="vertical";
          id="channel_setup";
          layout_width="340dp";
          layout_height="120dp";
          layout_gravity="center";
          {
            LinearLayout;
            orientation="horizontal";
            layout_width="fill";
            layout_marginTop="5dp";
            layout_marginLeft="5dp";
            {
              CardView;
              layout_margin="10dp";
              CardElevation="0dp";
              layout_width="40dp";
              layout_height="40dp";
              radius="10dp";
              {
                ImageView;
                layout_width="40dp";
                layout_height="40dp";
                src="res/icon/miniworld.png";
              };
            };
            {
              LinearLayout;
              orientation="vertical";
              layout_width="fill";
              gravity="center|left";
              layout_gravity="center";
              {
                TextView;
                textSize="16sp";
                text="迷你世界";
                textColor="#000000";
              };
              {
                TextView;
                textSize="10dp";
                id="miniworld_package_name";
                text="包名：未知";
              };
            };
          };
          {
            LinearLayout;
            layout_width="fill";
            layout_marginLeft="5dp";
            {
              TextView;
              layout_margin="10dp";
              textColor="#2196F3";
              text="版本：未知";
              layout_marginTop="0dp";
              id="miniworld_version";
            };
            {
              TextView;
              layout_margin="10dp";
              textColor="#00C853";
              text="安卓版本：未知";
              layout_marginTop="0dp";
              id="android_version";
            };
          };
        };
        {
          ImageView;
          layout_gravity="center|right";
          layout_height="20dp";
          layout_width="20dp";
          layout_marginRight="20dp";
          src="res/icon/switch.png";
        };
      };
      {
        CardView;
        layout_margin="20dp";
        radius="20dp";
        layout_marginTop="0dp";
        background="#FFFFFF";
        layout_width="320dp";
        {
          LinearLayout;
          layout_width="fill";
          layout_height="fill";
          orientation="vertical";
          {
            CardView;
            CardElevation="0dp";
            radius="0dp";
            layout_width="fill";
            layout_height="60dp";
            background="#FFFFFF";
            {
              LinearLayout;
              style="?android:attr/buttonBarButtonStyle";
              id="miniworld_account";
              layout_width="340dp";
              layout_height="80dp";
              layout_gravity="center";
            };
            {
              LinearLayout;
              layout_height="fill";
              {
                ImageView;
                layout_gravity="center|left";
                layout_marginLeft="35px";
                layout_width="25dp";
                layout_height="25dp";
                src="res/icon/mini.png";
              };
              {
                TextView;
                layout_gravity="center|left";
                layout_marginLeft="15dp";
                textSize="16sp";
                text="迷你号";
                textColor="#000000";
              };
            };
            {
              LinearLayout;
              layout_gravity="right";
              layout_height="fill";
              gravity="center";
              {
                TextView;
                id="miniworld_Account";
                text="未设置";
              };
              {
                ImageView;
                layout_marginLeft="10dp";
                layout_height="20dp";
                layout_width="20dp";
                layout_marginRight="15dp";
                src="res/icon/switch.png";
              };
            };
          };
          {
            CardView;
            CardElevation="0dp";
            radius="0dp";
            layout_width="fill";
            layout_height="60dp";
            background="#FFFFFF";
            {
              LinearLayout;
              style="?android:attr/buttonBarButtonStyle";
              id="software_mode";
              layout_width="340dp";
              layout_height="80dp";
              layout_gravity="center";
            };
            {
              LinearLayout;
              layout_height="fill";
              {
                ImageView;
                layout_gravity="center|left";
                layout_marginLeft="35px";
                layout_width="25dp";
                layout_height="25dp";
                src="res/icon/adjust.png";
              };
              {
                TextView;
                layout_gravity="center|left";
                layout_marginLeft="15dp";
                textSize="16sp";
                text="软件运行模式";
                textColor="#000000";
              };
            };
            {
              LinearLayout;
              layout_gravity="right";
              layout_height="fill";
              gravity="center";
              {
                TextView;
                id="software_Mode";
                text="未设置";
              };
              {
                ImageView;
                layout_marginLeft="10dp";
                layout_height="20dp";
                layout_width="20dp";
                layout_marginRight="15dp";
                src="res/icon/switch.png";
              };
            };
          };
          {
            CardView;
            CardElevation="0dp";
            radius="0dp";
            layout_width="fill";
            layout_height="60dp";
            background="#FFFFFF";
            {
              LinearLayout;
              style="?android:attr/buttonBarButtonStyle";
              id="software_about";
              layout_width="340dp";
              layout_height="80dp";
              layout_gravity="center";
            };
            {
              LinearLayout;
              layout_height="fill";
              {
                ImageView;
                layout_gravity="center|left";
                layout_marginLeft="35px";
                layout_width="25dp";
                layout_height="25dp";
                src="res/icon/prompt.png";
              };
              {
                TextView;
                layout_gravity="center|left";
                layout_marginLeft="15dp";
                textSize="16sp";
                text="关于与帮助";
                textColor="#000000";
              };
            };
          };
          {
            CardView;
            CardElevation="0dp";
            radius="0dp";
            layout_width="fill";
            layout_height="60dp";
            background="#FFFFFF";
            {
              LinearLayout;
              style="?android:attr/buttonBarButtonStyle";
              id="software_setting";
              layout_width="340dp";
              layout_height="80dp";
              layout_gravity="center";
            };
            {
              LinearLayout;
              layout_height="fill";
              {
                ImageView;
                layout_gravity="center|left";
                layout_marginLeft="35px";
                layout_width="25dp";
                layout_height="25dp";
                src="res/icon/setting.png";
              };
              {
                TextView;
                layout_gravity="center|left";
                layout_marginLeft="15dp";
                textSize="16sp";
                text="设置";
                textColor="#000000";
              };
            };
          };
        };
      };
    };
  };
}
